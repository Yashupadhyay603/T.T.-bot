<robot name="slider_bot">
    
<material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

<link name="base">
  <inertial>
         <origin rpy="0 0 0" xyz="0 0 0"/>
         <mass value="100"/>
         <inertia
           ixx="0.3854"
           iyy="13.875"
           izz="13.51"
           ixy="0.0"
           ixz="0.0"
           iyz="0.0" />
        </inertial>
    <visual>
      <geometry>
        <box size="0.9 0.05 0.008"/>
      </geometry>
           <material name="white"/>
      <origin rpy="0 0 0" xyz="0 -0.525 0"/>
    </visual>
    <collision>
<origin rpy="0 0 0" xyz="0 -0.525 0"/>
        <geometry>
        <box size="0.9 0.05 0.008"/>
      </geometry>
    </collision>
    <contact>
      <lateral_friction value="0.01"/>
      <spinning_friction value="0.001"/>
    </contact>
</link>
<link name="base_R">
<inertial>
         <origin rpy="0 0 0" xyz="0 0 0"/>
         <mass value="100"/>
         <inertia
           ixx="0.3854"
           iyy="13.875"
           izz="13.51"
           ixy="0.0"
           ixz="0.0"
           iyz="0.0" />
        </inertial>
    <visual>
      <geometry>
        <box size="0.9 0.05 0.008"/>
      </geometry>
           <material name="white"/>
      <origin rpy="0 0 0" xyz="0 0.525 0"/>
    </visual>
    <collision>
<origin rpy="0 0 0" xyz="0 0.525 0"/>
        <geometry>
        <box size="0.9 0.05 0.008"/>
      </geometry>
    </collision>
        <contact>
      <lateral_friction value="0.01"/>
      <spinning_friction value="0.001"/>
    </contact>
</link>
<joint name="base_L_to_base_R" type="fixed">
<parent link="base"/>
  <child link="base_R"/>
<origin xyz="0 0 0"/>
</joint>
<link name="slider">
<inertial>
         <origin xyz ="0 0 0.023" rpy="0 0 0"/>
         <mass value="5"/>
         <inertia
           ixx="0.2834"
           iyy="0.750"
           izz="0.883"
           ixy="0.0"
           ixz="0.0"
           iyz="0.0" />
        </inertial>
   <visual>
     <geometry>
        <box size="0.08 1.05 0.02"/>
     </geometry>
     <origin xyz ="0 0 0.023" rpy="0 0 0"/>
     <material name="blue"/>
   </visual>
    <collision>
<origin xyz ="0 0 0.023" rpy="0 0 0"/>
        <geometry>
        <box size="0.08 1.05 0.02"/>
     </geometry>

    </collision>
    <contact>
      <lateral_friction value="0.01"/>
      <spinning_friction value="0.001"/>
    </contact>
</link>
<joint name="base_to_slider" type="prismatic">
  <axis xyz="1 0 0"/>
        <limit effort="100.0" lower="-0.38" upper="0.38" velocity="0.9"/>
<parent link="base"/>
  <child link="slider"/>
<origin xyz="0 0 0"/>
</joint>
<link name="hockey">
        <inertial>
         <origin xyz ="0 0 -0.065" rpy="0 0 0"/>
         <mass value="0.2"/>
         <inertia
           ixx="0.00038"
           iyy="0.00038"
           izz="0.00075"
           ixy="0.0"
           ixz="0.0"
           iyz="0.0" />
        </inertial>
        <visual>
          <origin xyz ="0 0 -0.065" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.04" radius="0.08"/>
        </geometry>
        <material name="white">
        </material>
      </visual>
      <collision>
            <origin xyz="0 0 -0.065" rpy="0 0 0"/>
            <geometry>
              <cylinder length="0.04" radius="0.08"/>
            </geometry>
      </collision>
   </link>
<link name="extension">
        <inertial>
         <origin xyz ="0 0 -0.02" rpy="0 0 0"/>
         <mass value="1"/>
         <inertia
           ixx="0.00038"
           iyy="0.00038"
           izz="0.00075"
           ixy="0.0"
           ixz="0.0"
           iyz="0.0" />
        </inertial>
        <visual>
          <origin xyz ="0 0 -0.02" rpy="0 0 0"/>
        <geometry>
          <cylinder length="0.08" radius="0.015"/>
        </geometry>
        <material name="white">
        </material>
      </visual>
      <collision>
            <origin xyz="0 0 -0.02" rpy="0 0 0"/>
            <geometry>
              <cylinder length="0.08" radius="0.015"/>
            </geometry>
      </collision>
   </link>
<joint name="extension_to_hockey" type="fixed">
<parent link="extension"/>
  <child link="hockey"/>
<origin xyz="0 0 0"/>
</joint>
<joint name="slider_to_extension" type="prismatic">
  <axis xyz="0 1 0"/>
        <limit effort="100.0" lower="-0.44" upper="0.44" velocity="0.9"/>
<parent link="slider"/>
  <child link="extension"/>
<origin xyz="0 0 -0.02"/>
</joint>
</robot>
